{
  "name": "nuxt-boilerplate",
  "version": "0.0.1",
  "description": "Nuxt.js boilerplate with bootstrap-vue.",
  "author": "Eldar <eldar@ministryofprogramming.com>",
  "private": true,
  "scripts": {
